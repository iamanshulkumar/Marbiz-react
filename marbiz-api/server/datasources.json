{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "mongoDB": {
    "url": "mongodb://localhost:27017/moverstrip",
    "name": "mongoDB",
    "useNewUrlParser": true,
    "connector": "mongodb"
  },
  "awsStorage": {
    "name": "awsStorage",
    "connector": "loopback-component-storage",
    "provider": "amazon",
    "key": "V4E2clppbcYpudfYxBSLHWrqwUd726OwKdWKHA2M",
    "keyId": "AKIATBXEGTMEJYLF6EPK"
  },
  "geoRest": {
    "name": "geoRest",
    "baseURL": "http://maps.googleapis.com/maps/api/geocode/{format=json}",
    "crud": false,
    "connector": "rest",
    "headers": {
      "accepts": "application/json",
      "content-type": "application/json"
    },
    "query": {
      "address": "{street},{city},{zipcode}",
      "sensor": "{sensor=false}"
    },
    "responsePath": "$.results[0].geometry.location"
  },
  "geoMatrixService": {
    "name": "geoMatrixService",
    "connector": "rest",
    "crud": false,
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://maps.googleapis.com/maps/api/distancematrix/json?origins={from}&destinations={to}&departure_time=now&key=AIzaSyBxgrIOwPu6fdAgDefZ0jE8knqjHT5gme8",
          "query": {
            "from": "{from}",
            "to": "{to}"
          }
        },
        "functions": {
          "getRoadDistance": ["from","to"]
        }
      }
    ],
    "headers": {
      "accepts": "application/json",
      "content-type": "application/json"
    }
  },
  "loconavService": {
    "name": "loconavService",
    "headers": {
      "accepts": "application/json",
      "Authorization": "qB7MVAJZznyfGq-2Ye5F",
      "content-type": "application/json"
    },
    "crud": false,
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://loconav.com/api/v3/device/all_devices?start_index={start_index}&end_index={end_index}"
        },
        "functions": {
          "findAllDevices": ["start_index","end_index"]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "http://loconav.com/api/v3/device/all_devices_with_count"
        },
        "functions": {
          "findAllDeviceCounts": []
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "http://loconav.com/api/v3/vehicle/distance_travelled",
          "query": {
            "start_time": "{start_time}",
            "end_time": "{end_time}",
            "vehicle_number": "{vehicle_number}"
          }
        },
        "functions": {
          "findVehicleDistanceTravelled": [
            "start_time",
            "end_time",
            "vehicle_number"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "http://loconav.com/api/v3/device/all_devices_with_count"
        },
        "functions": {
          "findAllDevicesWithCount": []
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "https://loconav.com/api/v3/truck/immobilize",
          "query": {
            "vehicle_number": "{vehicle_number}"
          }
        },
        "functions": {
          "requestLockVehicle": [
            "vehicle_number"
          ]
        }
      },
      { 
        "template": {
          "method": "GET",
          "url": "https://loconav.com/api/v3/truck/mobilize",
          "query": {
            "vehicle_number": "{vehicle_number}"
          }
        },
        "functions": {
          "requestUnlockVehicle": [
            "vehicle_number"
          ]
        }
      }
    ]
  },
  "smsService": {
    "name": "smsService",
    "headers": {
      "accepts": "application/json",
      "content-type": "application/json"
    },
    "crud": false,
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://sms.jaipursmshub.in/api_v2/message/send?api_key=uJXTLH2O_xV_r5utjRB0SEW8rrLBSy9APxqM_sB-KL7bR-_bL8vULpQtBdBMZWKC&sender_id=MTGPSA&dlt_template_id={dlt_template_id}",
          "query": {
            "mobile_no": "{mobile_no}",
            "message": "{message}"
          }
        },
        "functions": {
          "sendSMS": [
            "mobile_no",
            "message",
            "dlt_template_id"
          ]
        }
      }
    ]
  },
  "googleMapService": {
    "name": "googleMapService",
    "headers": {
      "accepts": "application/json",
      "content-type": "application/json"
    },
    "crud": false,
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://maps.googleapis.com/maps/api/distancematrix/json",
          "query": {
            "destinations": "{destinations}",
            "key": "AIzaSyBxgrIOwPu6fdAgDefZ0jE8knqjHT5gme8",
            "departure_time": "now",
            "origins": "{origins}"
          }
        },
        "functions": {
          "getDistance": [
            "destinations",
            "origins"
          ]
        }
      }
    ]
  }
}