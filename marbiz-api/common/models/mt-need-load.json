{
    "name": "MtNeedLoad",
    "base": "PersistedModel",
    "strictObjectIDCoercion": true,
    "idInjection": true,
    "options": {
      "validateUpsert": true
    },
    "properties": {
      "fromLocation": {
        "type": "string",
        "required": true
      },
      "toLocation": {
        "type": "any",
        "required": true
      },
      "vehicleNumber": {
        "type": "string",
        "required": true
      },
      "itemWeight": {
        "type": "string"
      },
      "contactNumber": {
        "type": "string"
      },
      "contactName": {
        "type": "string"
      },
      "mtUserId": {
        "type": "string"
      },
      "createdAt": {
        "type": "date",
        "defaultFn":"now"
      }
    },
    "validations": [],
    "relations": {},
    "acls": [],
    "methods": {
      "getArchiveData": {
        "accepts": [
          {
            "arg": "days",
            "type": "number"
          }
        ],
        "returns": {
          "type": "object",
          "arg": "response",
          "root": true
        },
        "description": "Get All NeedLoad Archive Data",
        "http": [
          {
            "path": "/getArchiveData",
            "verb": "get"
          }
        ]
      }
    }
  }
  