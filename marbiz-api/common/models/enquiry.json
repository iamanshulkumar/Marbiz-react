        "profileId": {
            "type": "string",
            "required": true
        },
        "occasion": {
            "type": "string",
            "required": true
        },
        "date": {
            "type": "date",
            "required": true
        },
        "city": {
            "type": "string",
            "required": true
        },
        "budget": {
            "type": "string"
        },
        "peopleAttend": {
            "type": "string"
        },
        "name": {
            "type": "string",
            "required": true
        },
        "email": {
            "type": "string"
        },
        "mobileNumber": {
            "type": "string"
        },
        "details": {
            "type": "string"
        },
        "moreOptionsBudget": {
            "type": "string",
