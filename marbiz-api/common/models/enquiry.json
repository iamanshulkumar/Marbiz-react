        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW"
        }
    ],
    "methods": {
        "pingMe": {
            "accepts": [],
            "returns": [
                {
                    "arg": "response",
                    "type": "string"
                }
            ],
            "description": "ping service",
            "http": [
                {
                    "path": "/pingMe",
                    "verb": "get"
                }
            ]
        }
